{
  "editor.formatOnSave": true,
  "cSpell.words": ["bpxdy", "studybud", "txwa", "wnrdn", "wqfd"],
  "files.associations": {
    "**/*.html": "html",
    "**/templates/*/*.html": "django-html",
    "**/templates/*": "django-html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements"
  },
  "[django-html]": {
    "editor.defaultFormatter": "batisteo.vscode-django"
  }
}
